{
    "name": "ohseesoftware/laravel-assert-encrypted",
    "description": "Add an assertion to test for encrypted values in your database.",
    "keywords": [
        "ohseesoftware",
        "laravel"
    ],
    "homepage": "https://github.com/ohseesoftware/laravel-assert-encrypted",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Owen Conti",
            "email": "owen@ohseemedia.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0",
        "php-coveralls/php-coveralls": "^2.2",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "OhSeeSoftware\\LaravelAssertEncrypted\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhSeeSoftware\\LaravelAssertEncrypted\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
